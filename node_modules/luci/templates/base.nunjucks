{% import 'components/luci.nunjucks' as luci with context %}
<html>
    <head>
        <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700" rel="stylesheet">
        <link rel="stylesheet" href="/styles/dependencies/dialog-polyfill.css">
        <link rel="stylesheet" href="/styles/luci.css">
        <script src="/scripts/dependencies/svg4everybody.min.js"></script>
        <!-- The datables plugin requires jQuery -->
        <script src="/scripts/dependencies/jquery.min.js"></script>
        <script src="/scripts/dependencies/jquery.dataTables.min.js"></script>
        <script src="/scripts/dependencies/ColReorderWithResize.js"></script>
        <script>
            svg4everybody();
        </script>
        {% block head_append %}
        {% endblock %}
    </head>
    <body>
        {% block notification %}
        {% endblock %}

        {% block body %}
        {% endblock %}
        <script src="/scripts/dependencies/dialog-polyfill.js" charset="utf-8"></script>
        <script src="/scripts/dependencies/interact.min.js" charset="utf-8"></script>
        <script src="/scripts/luci.js"></script>
    </body>
</html>
