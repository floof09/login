    <script>
      "use strict";

      function luciLink() {
          const luciLink = document.querySelectorAll('.luci-link');
          var i,
              randomNumber;

          for (i = 0; i < luciLink.length; i++) {
              randomNumber = Math.floor(Math.random() * 1000000);
              luciLink[i].href='#' + randomNumber;
          }
      }

      document.addEventListener('DOMContentLoaded', function() {
          luciLink();
      });
    </script>
